	    <include refid="Base_Column_List" />
	    from T_WMS_INVENTORY_LINE 
	    where 1=1
    <if test="inventoryid != null and inventoryid != ''" >
        and inventoryid=#{inventoryid}
      </if>
    <if test="areaid != null and areaid != ''" >
        and areaid=#{areaid}
      </if>
